{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:react/jsx-runtime"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "semi": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "object-curly-newline": 0,
    "react/jsx-one-expression-per-line": 0
  }
}